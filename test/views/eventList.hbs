<div class="row">
    <div class="col">
        <nav class="navbar navbar-dark default-color justify-content-between">
            <a class="navbar-brand" href="/">Event On Fire</a>

        </nav>
    </div>
</div>



<div class="row no-gutters">


    {{#each events}}
    <div class="col " style=" border-right: 1px solid #333;">

        <div class="left d-flex  ">
            <div class="card" id="card1" style="margin-left: 50px;">

                <!-- Card image -->
                <div class="view overlay">
                    <img class="card-img-top"
                        src="https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcSn9hLCvqbX-a24aSGvTq91V_f-HzrD6CMTmrDespCPW9dE-tJC"
                        alt="Card image cap">
                    <a href="#!">
                        <div class="mask rgba-white-slight"></div>
                    </a>
                </div>

                <!-- Card content -->
                <div class="card-body">

                    <!-- Title -->
                    <h4 class="card-title">
                        <p style="float: left;"> <b style="color: #2bbbad;">Event Name-</b></p> {{this.eventName}}
                    </h4>
                    <hr>

                    <h6 class="card-title"><b style="color:red">Category-</b> {{this.category}}</h6>
                    <h6 class=" card-title"> <b style="color:red">Starting Date-</b> {{this.date}}</h6>
                    <h6 class="card-title"> <b style="color:red">Event Place-</b> {{this.place}}</h6>
                    <!-- Text -->
                    <p><b style="color:red">About Event-</b>{{this.descirption}}</p>

                </div>

            </div>

        </div>

    </div>
    {{/each}}
</div>
<!-- Footer -->
<footer class="page-footer font-small black darken-3">

    <!-- Footer Elements -->
    <div class="container">

        <!-- Grid row-->
        <div class="row">

            <!-- Grid column -->
            <div class="col-md-12 py-5">
                <div class="mb-5 flex-center">

                    <!-- Facebook -->
                    <a class="fb-ic">
                        <i class="fab fa-facebook-f fa-lg white-text mr-md-5 mr-3 fa-2x"> </i>
                    </a>
                    <!-- Twitter -->
                    <a class="tw-ic">
                        <i class="fab fa-twitter fa-lg white-text mr-md-5 mr-3 fa-2x"> </i>
                    </a>
                    <!-- Google +-->
                    <a class="gplus-ic">
                        <i class="fab fa-google-plus-g fa-lg white-text mr-md-5 mr-3 fa-2x"> </i>
                    </a>
                    <!--Linkedin -->
                    <a class="li-ic">
                        <i class="fab fa-linkedin-in fa-lg white-text mr-md-5 mr-3 fa-2x"> </i>
                    </a>
                    <!--Instagram-->
                    <a class="ins-ic">
                        <i class="fab fa-instagram fa-lg white-text mr-md-5 mr-3 fa-2x"> </i>
                    </a>
                    <!--Pinterest-->
                    <a class="pin-ic">
                        <i class="fab fa-pinterest fa-lg white-text fa-2x"> </i>
                    </a>
                </div>
            </div>
            <!-- Grid column -->

        </div>
        <!-- Grid row-->

    </div>
    <!-- Footer Elements -->

    <!-- Copyright -->
    <div class="footer-copyright text-center py-3">Â© 2020 Copyright:
        <a href="https://mdbootstrap.com/">eventonfire.com</a>
    </div>
    <!-- Copyright -->

</footer>
